{
  "manifest_version": 2,

  "name": "SpeedDial DEV",
  "description": "View emergency numbers of your state in the omnibox.",
  "version": "0.0.5.3",
  "author": "Vivek Bhookya, Deepika Gudavalli",

  "browser_action": {
    "default_icon": "icons/heart-icon-48.png",
    "default_popup": "popup.html",
    "default_title": "SpeedDial"
  },

  /* "icons": { "16": "icons/heart-icon-16.png",
             "48": "icons/heart-icon-48.png",
             "128": "icons/heart-icon-128.png" }, */

  "background": {
      "scripts": ["jquery.min.js", "linkManager.js"],
      "persistent": false
      },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.min.js", "linkManager.js"],
      "run_at": "document_start"
    }
  ],

  "permissions": [
    "activeTab",
    "storage"
  ]
}
